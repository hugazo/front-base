{
  "name": "nuxt-demo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "image:build": "docker build -t registry.gitlab.com/42devs-core/projects/front-base .",
    "image:push": "docker push registry.gitlab.com/42devs-core/projects/front-base",
    "image:clean": "docker image prune -f",
    "image:run": "docker run -it --rm -d -p 8080:80 --name 42web docker push registry.gitlab.com/42devs-core/projects/front-base"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.0",
    "@nuxtjs/bulma": "^1.3.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "nuxt": "^2.14.0",
    "nuxt-logger": "^0.2.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-nuxt": "^1.0.1",
    "eslint-plugin-nuxt": "^1.0.0",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0"
  },
  "peerDependencies": {
    "eslint-plugin-import": "^2.22.0"
  }
